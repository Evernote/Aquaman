{
  "name": "aquaman-redux",
  "version": "0.1.4",
  "description": "Redux middlewear for composable, declarative control flow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build-module": "webpack webpack.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:types && npm run build-module",
    "test": "jest",
    "prepare": "npm run build",
    "ts": "tsc --noEmit"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evernote/Aquaman.git"
  },
  "keywords": [
    "redux"
  ],
  "author": "Evernote",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "jsdom": "^15.1.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "redux": "^4.0.4",
    "rxjs": "^6.5.2"
  }
}
