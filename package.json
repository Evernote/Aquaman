{
  "name": "aquaman-redux",
  "version": "1.0.0",
  "description": "Redux middlewear for composable, declarative control flow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build-module": "webpack --config webpack.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:types && npm run build-module",
    "test": "jest",
    "prepare": "npm run build",
    "ts": "tsc --noEmit"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evernote/Aquaman.git"
  },
  "keywords": [
    "redux"
  ],
  "author": "Evernote",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-async-generator-functions": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.15",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "redux": "^4.0.5",
    "rxjs": "^6.6.3"
  },
  "peerDependencies": {
    "redux": ">4",
    "rxjs": ">6"
  }
}
